#include "test.h"

#test check_memcpy
	const char *src = "42 is a school.";
	char dest[50];
	char dest2[50];

	printf("CHECKING MEMCPY\n");
	memcpy(dest, src, strlen(src) + 1);
	ft_memcpy(dest2, src, strlen(src) + 1);
	ck_assert_str_eq(dest, src);
	ck_assert_str_eq(dest2, src);
	ck_assert_str_eq(dest, dest2);
